#twisted prime checker

def is_prime(num):
    for i in range(2, int((num/2)+1)):
        if num%i==0:
            return False
    return True
num=int(input("number\n"))
numrev=int(str(num)[::-1])
if is_prime(num) and is_prime(numrev):
    print("num is twisted prime")
else:
    print("not a twisted prime")
